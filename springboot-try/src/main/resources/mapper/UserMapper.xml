<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="zhuxuanyu.mapper.UserMapper">

    <resultMap id="BaseResultMap" type="zhuxuanyu.pojo.User">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="username" column="username" jdbcType="VARCHAR"/>
            <result property="password" column="password" jdbcType="VARCHAR"/>
            <result property="gender" column="gender" jdbcType="CHAR"/>
            <result property="sign" column="sign" jdbcType="VARCHAR"/>
            <result property="state" column="state" jdbcType="TINYINT"/>
            <result property="head" column="head" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,username,password,
        gender,sign,state,
        head
    </sql>
    <insert id="insertSelective">
        insert into t_user
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">id,</if>
            <if test="username != null">username,</if>
            <if test="password != null">password,</if>
            <if test="gender != null">gender,</if>
            <if test="sign != null">sign,</if>
            <if test="state != null">state,</if>
            <if test="head != null">head,</if>
        </trim>
        values
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id,jdbcType=INTEGER},</if>
            <if test="username != null">#{username,jdbcType=VARCHAR},</if>
            <if test="password != null">#{password,jdbcType=VARCHAR},</if>
            <if test="gender != null">#{gender,jdbcType=CHAR},</if>
            <if test="sign != null">#{sign,jdbcType=VARCHAR},</if>
            <if test="state != null">#{state,jdbcType=TINYINT},</if>
            <if test="head != null">#{head,jdbcType=VARCHAR},</if>
        </trim>
    </insert>
    <delete id="deleteById">
        delete
        from t_user
        where id = #{id,jdbcType=NUMERIC}
    </delete>
</mapper>
